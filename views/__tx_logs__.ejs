<div>

    <div>

        <p class='text-left px-6 py-12 text-2xl font-bold text-white'>
            Transaction log </p>
        <table class='w-full border border-gray-600  '>
            <tr class='text-center text-lg text-white  font-bold'>
                <th class='py-6'>
                    Date
                </th>
                <th>
                    Transaction ID
                </th>
                <th>
                    Amount
                </th>

                <th>
                    Details
                </th>
            </tr>

[/ transactions.map((t)=>{ /]

            <tr
                class='text-center  hover:bg-gray-200 cursor-pointer text-base text-black bg-gray-100'>
                <td class="py-4 PX-2"> [/= t.date.toLocaleString() /] </td>
                <td class='px-2 '> [/=t.ref/] </td>
                <td class='px-2'> $[/=t.amount/] </td>
                <td class='text-sm overflow-x-scroll px-2 '> [/=t.details/] </td>
            </tr>

            [/ }) /]


        </table>
    </div>

</div>